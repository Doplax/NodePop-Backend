{
  "name": "nodeapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/bin/www",
    "dev": "cross-env NODE_ENV=development DEBUG=nodeapp:* nodemon --exec ts-node ./index.ts",
    "build": "tsc --build .",
    "dev:cluster": "cross-env DEBUG=nodeapp:* nodemon --inspect --exec ts-node ./index.ts",
    "init-db": "ts-node ./src/db/init-db.ts",
    "init-docker": "docker-compose up -d",
    "email-sender-service": "ts-node ./micro-services/emailSender.ts"
  },
  "dependencies": {
