    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/express-validator": "^2.20.33",
    "@types/i18n": "^0.13.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose-delete": "^1.0.3",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
